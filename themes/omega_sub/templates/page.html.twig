<header>
  {{page.header}}
  <div id="header_overlay" class="overlay"></div>
</header>
<div id="about">
  {{page.about}}
</div>
<div id="video">
  {{page.video}}
  <div id="video_overlay" class="overlay"></div>
</div>
<div id="expertise">
  {{page.expertise}}
</div>
<div id="team">
  {{page.team}}
  <div id="team_overlay" class="overlay"></div>
</div>
<div id="works">
  {{page.works}}
</div>
<div id="testimonials">
  {{page.testimonials}}
  <div id="testimonials_overlay" class="overlay"></div>
</div>
<div id="contact">
  {{page.contact}}
</div>
<footer>
  {{page.footer}}
  <div id="footer_overlay" class="overlay"></div>
</footer>

<section id="content-outer-wrapper" class="outer-wrapper clearfix">
  <div id="main-layout" class="{{ region_classes.main }} inner-wrapper clearfix">
    {% if page.content %}
      {{ page.content }}
    {% endif %}
  </div><!-- /#main -->
</section><!-- /#main-outer-wrapper -->
