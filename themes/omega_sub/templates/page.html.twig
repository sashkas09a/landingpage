
<section id="content-outer-wrapper" class="outer-wrapper clearfix">
  <div id="main-layout" class="{{ region_classes.main }} inner-wrapper clearfix">

    {% if page.content %}
      {{ page.content }}
      <header>
        {{page.header}}
      </header>
      <div id="about">
        {{page.about}}
      </div>
      <div id="video">
        {{page.video}}
      </div>
      <div id="expertise">
        {{page.expertise}}
      </div>
      <div id="team">
        {{page.team}}
      </div>
      <div id="works">
        {{page.works}}
      </div>
      <div id="testimonials">
        {{page.testimonials}}
      </div>
      <div id="contact">
        {{page.contact}}
      </div>
      <footer>
        {{page.footer}}
      </footer>

    {% endif %}



  </div><!-- /#main -->
</section><!-- /#main-outer-wrapper -->

